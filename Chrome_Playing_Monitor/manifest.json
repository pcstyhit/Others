{
    "manifest_version": 3,
    "name": "Check Media Playback",
    "version": "1.0",
    "permissions": [
      "tabs",
      "scripting",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  
